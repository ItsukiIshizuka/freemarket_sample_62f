<%= form_for @product, url: products_path, method: :post do |f| %>
  <%= f.label :name, "商品名"%>
  <%= f.text_field :name %>
  <%= f.label :name, "説明"%>
  <%= f.text_field :information %>
  <%= f.label :name, "値段"%>
  <%= f.text_field :price %>
  <%= f.label :name, "地域"%>
  <%= f.text_field :area %>
  <%= f.label :name, "状態"%>
  <%= f.text_field :status %>
  <%= f.label :name, "発送までの日数"%>
  <%= f.text_field :sending_days %>
  <%= f.label :name, "利益"%>
  <%= f.text_field :profit %>
  <%= f.label :name, "販売状況"%>
  <%= f.text_field :selling_status %>
  <%= f.submit %>

<% end %>